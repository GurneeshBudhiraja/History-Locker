{
  "manifest_version": 3,
  "name": "Youtube Bookmarker",
  "description": "Bookmarks the videos you watch on YouTube.",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts":[
    {
      "matches": ["https://*.youtube.com/*"],
      "js": ["./popup.js"]
    }
  ],
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": ["https://*.youtube.com/*"]
}
